{
  "crons": [
    {
      "path": "/api/cron/every-minute",
      "schedule": "* * * * *"
    },
    {
      "path": "/api/cron/every-day",
      "schedule": "0 18 * * *"
    }
  ],
  "rewrites": [
    {
      "source": "/:ref_id*",
      "has": [
        {
          "type": "host",
          "value": "ahoy.hack.club"
        }
      ],
      "destination": "/?ref=:path*"
    }
  ]
}
